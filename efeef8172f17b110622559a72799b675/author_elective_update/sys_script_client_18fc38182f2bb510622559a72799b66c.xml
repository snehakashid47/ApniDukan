    <sys_script_client action="DELETE">
        <active>true</active>
        <applies_extended>false</applies_extended>
        <condition/>
        <description/>
        <field>pincode</field>
        <global>true</global>
        <isolate_script>true</isolate_script>
        <messages/>
        <name>City changes as per pincode</name>
        <order/>
        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading) {
    if (isLoading || newValue === '') {
        return;
    }

    var pincode = newValue;

    // Query postaldata table based on pincode
    var postalDataGR = new GlideRecord('postaldata');
    postalDataGR.addQuery('pincode', pincode);
    postalDataGR.query(function (result) {
        if (result.next()) {
            // Set the city field in the order form
            g_form.setValue('city', postalDataGR.city);
        }
    });
}
]]></script>
